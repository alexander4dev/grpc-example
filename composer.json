{
    "type": "project",
    "name": "sunrise/awesome-skeleton-roadrunner",
    "description": "Awesome Skeleton for modern development on PHP 7.1+ and RoadRunner",
    "keywords": ["fenric", "sunrise", "skeleton", "psr-2", "psr-3", "psr-7", "psr-11", "psr-15", "psr-17", "roadrunner", "golang"],
    "homepage": "https://github.com/sunrise-php/awesome-skeleton-roadrunner",
    "license": "MIT",
    "authors": [{
        "name": "Anatoly Fenric",
        "email": "anatoly@fenric.ru",
        "homepage": "https://anatoly.fenric.ru/"
    }],
    "require": {
        "php": "^7.1",
        "arus/php-coding-standard": "^1.0",
        "arus/api-foundation": "^3.1",
        "arus/doctrine-helper-kit": "^1.0",
        "arus/doctrine-persistent-entity-manager-middleware": "^1.0",
        "arus/payload-validation-middleware": "^1.0",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "middlewares/encoder": "^1.1",
        "middlewares/payload": "^2.1",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "sunrise/http-factory": "^1.0",
        "sunrise/http-router": "^1.0",
        "sunrise/http-router-annotations-support": "^1.0",
        "spiral/roadrunner": "^1.3",
        "symfony/console": "^4.2",
        "symfony/validator": "^4.2",
        "arus/doctrine-repository-factory": "^1.0",
        "zircote/swagger-php": "^3.0",
        "zendframework/zend-inputfilter": "^2.10",
        "doctrine/migrations": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always --coverage-text",
            "phpcs"
        ]
    },
    "repositories": {
        "arus/api-foundation" : {
            "type": "git",
            "url": "git@gitlab.voshod.local:web/http-api-foundation.git",
            "trunk-path": "master"
        },
        "arus/doctrine-helper-kit" : {
            "type": "git",
            "url": "git@gitlab.voshod.local:web/doctrine-helper-kit.git",
            "trunk-path": "master"
        },
        "arus/payload-validation-middleware" : {
            "type": "git",
            "url": "git@gitlab.voshod.local:web/payload-validation-middleware.git",
            "trunk-path": "master"
        }
    }
}
